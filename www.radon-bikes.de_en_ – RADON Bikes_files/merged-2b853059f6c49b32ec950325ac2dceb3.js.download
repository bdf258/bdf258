                                    saleBadge.find('span').text('');
                                }

                                if (colorData.prices[currency].special_price != undefined) {
                                    priceActive.text(colorData.prices[currency].special_price + ' ' + colorData.prices[currency].currency_symbol);
                                    priceInactive.text(colorData.prices[currency].price + ' ' + colorData.prices[currency].currency_symbol);
                                } else {
                                    priceActive.text(colorData.prices[currency].price + ' ' + colorData.prices[currency].currency_symbol);
                                }

                                if (currency == 'eur') {
                                    self.elm.$buyRate.html(Math.ceil((priceActive.html().replace('.', '').replace('€', ''))/12));
                                }

                            }
