                        // updated prices and save badge
                        for (var currency in colorData.prices) {
                            if (colorData.prices.hasOwnProperty(currency)) {

                                var saleBadge = $('.a-badge--sale.currency_'+currency);
                                var priceActive = $('.m-bikedetail__price.currency_'+currency+' .m-bikedetail__price--active');
                                var priceInactive = $('.m-bikedetail__price.currency_'+currency+' .m-bikedetail__price--inactive:contains("UVP") .h-line-through');

