         * Customized Function
         */
        addVideo: function ($fallback) {
            var self = this,
                videoPath,
                viewport,
                viewportWidth = $(window).width();

            // determine current viewport
            if (viewportWidth <= mediaQueries.smartphone.maxWidth) {
                viewport = 'small';
            } else if (mediaQueries.tablet.minWidth <= viewportWidth && viewportWidth <= mediaQueries.desktop.maxWidth) {
                viewport = 'medium';
            } else if (mediaQueries.desktop.minWidth <= viewportWidth) {
                viewport = 'large';
            }

            videoPath = $fallback.data('videosrc-' + viewport);

            // Check if previous lazyload exists
            if ($fallback.hasClass('is-lazy-loaded')) {
                var previousLazyLoad = $fallback.data('prev-load'),
                    $video = $fallback.siblings('js-video-wrap').find('video');

                if (viewport == 'small' || (viewport == 'medium' && previousLazyLoad != 'small') || (viewport == 'large' && previousLazyLoad == 'large')) {
                    return $video;
                } else {
                    $fallback.data('prev-load', viewport);
                    $video.find('source').attr('src', videoPath);

                    if (viewportWidth > mediaQueries.tablet.maxWidth) {
                        $video.removeAttr('controls');
                        $video.attr('autoplay', true);
                    }
                    return $video;
                }

                // Otherwise add video to DOM
            } else {
                var $videoWrap = $('<div class="background-video js-video-wrap"></div>');
                var $video = $('<video class="js-resize-to-parent"><video/>');

                /*if (viewportWidth > mediaQueries.tablet.maxWidth) {*/
                $video.attr('autoplay', true);
                /*} else {
                    $video.attr('controls', true);
                }*/

                // cure iPad resizing
                var isiPad = navigator.userAgent.match(/iPad/i) != null;
                if (isiPad) $video.addClass('ipad');


                $video.attr('poster', '/typo3conf/ext/cccc_framework_styleguide/Resources/Public/img/transparent.png');
                $video.attr('loop', true);

                var $source = $('<source/>');
                $source.attr('src', videoPath);

                $video.append($source);
                $videoWrap.append($video);
                $fallback.after($videoWrap).addClass('is-lazy-loaded').data('prev-load', viewport);

                $video.on('load', function () {
                    $(window).trigger('resize');
                });

                ssm.addStates([
                    {
                        id: 'mobile',
                        query: '(max-width: ' + mediaQueries.smartphone.maxWidth + 'px)',
                        onEnter: function () {
                            $video.on('play', function () {
                                $fallback.siblings('.js-video-preloader').hide();
                            });
                        },
                        onLeave: function () {
                            $video.off('play');
                        }
                    },
                    {
                        id: 'table up',
                        query: '(min-width: ' + mediaQueries.tablet.minWidth + 'px)',
                        onEnter: function () {
                            $video.on('play', function () {
                                $fallback.siblings('.js-video-preloader').hide();
                                $(window).trigger('resize');
                            });
                        },
                        onLeave: function () {
                            $video.off('play');
                        }
                    }
                ]);

                $video.on('play', function () {
                    $fallback.siblings('.js-video-preloader').hide();
                    $(window).trigger('resize');
                });

                return $video;
            }
        }

    });
