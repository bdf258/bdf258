                    } else if (dataPhotoswipe.hasOwnProperty('tablet')) {
                        imageSize = 'tablet';
                    }
                } else if (viewportWidth < mediaQueries.tablet.maxWidth && dataPhotoswipe.hasOwnProperty('tablet')) {
                    imageSize = 'tablet';
