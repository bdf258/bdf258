            function calculateFramesize() {
                var height = $('#height').val().replace(",", ".");
                if (height == '')
                    return;
                if (isNaN(height)) {
                    alert(_js_trans['please_enter_a_number']);
                    return;
                } else if (height < 3) {
                    alert(_js_trans['value_too_small']);
                    return;
                }
                var biketype = $('#framesize-select').prop("selectedIndex");
                var cms, t1 = _js_trans['framesize_in_inches'], t2 = _js_trans['equates_to'], t3 = _js_trans['framesize_in_cm'];
                var factorinch = 0
                    , factorcm = 0;
                switch (biketype) {
                    case 0:
                        factorinch = 0.226;
                        break;
                    case 1:
                        factorinch = 0.226;
                        break;
                    case 2:
                        factorinch = 0.24;
                        factorcm = 0.61;
                        break;
                    case 3:
                        factorinch = 0.665;
                        factorcm = -4.25;
                        t1 = _js_trans['cm_racing_bike'];
                        t2 = _js_trans['framesize'];
                        t3 = _js_trans['cm_triathlon_bike'];
                        break;
                    case 4:
                        factorinch = 0.259;
                        factorcm = 0.66;
                        break;
                }
                var inches = Math.round(height * factorinch);
                if (factorcm == 0) {
                    cms = Math.round(inches * 2.56);
                } else if (factorcm < 0) {
                    cms = Math.round(inches - (-factorcm));
                } else {
                    cms = Math.round(height * factorcm);
                }
                $('#info_frame_inch').html(inches);
                $('#info_frame_cm').html(cms);
                $('#info_text_1').html(t1);
                $('#info_text_2').html(t2);
                $('#info_text_3').html(t3);

