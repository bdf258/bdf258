        self.settings.animationSpeed = 500;

        this.on = function (callback) {
            var self = this;

            self.$ctx.ccccToggle({
                toggleClass: 'is-open',
                toggleTarget: function () {
                    return self.$ctx;
                },
                on: 'offcanvasToggled',
                callback: function () {
                    self.toggleState();
                    self.fire('offcanvasToggled', {offcanvasId: self.$ctx.data('offcanvas-id')});
                }
            });

            // Close menu by clicking on any other element
            $('body').click(function (e) {
                // Check if the click was not on the menu itself && the menu is open
                if (!($(e.target).closest(self.$ctx).length) && self.state.isOpen) {
                    self.onOffcanvasToggleClicked({offcanvasId: self.$ctx.data('offcanvas-id')});
                }
            });






            //
            // change cookie and html class for currency
            //--------------------------------------------------------
            self.elm.$currencyLink.on('click', function(e) {
                e.preventDefault();

                var clickedCurrency = $(this).attr('data-currency');
                self.elm.$currencyLink.removeClass('current-currency');
                $(this).addClass('current-currency');

                $('html').removeClass('global-currency-eur global-currency-gbp');
                switch (clickedCurrency) {
                    case 'eur':
                        var now = new Date();
                        var currencyCookieExpire = now.getTime() + 1000 * 31536000;
                        now.setTime(currencyCookieExpire);
                        document.cookie = "currency=eur; expires=" + now.toGMTString() + "; path=/";
                        $('html').addClass('global-currency-eur');
                        break;
                    case 'gbp':
                        var now = new Date();
                        var currencyCookieExpire = now.getTime() + 1000*31536000;
                        now.setTime(currencyCookieExpire);
                        document.cookie = "currency=gbp; expires="+now.toGMTString()+"; path=/";
                        $('html').addClass('global-currency-gbp');
                        break;
                }
            });







            parent.on(callback);
        };

        this.after = function () {
            parent.after();
        };

        this.onOffcanvasToggleClicked = function (data) {
            var self = this,
                offcanvasId = data.offcanvasId;

            if (self.$ctx.data('offcanvas-id') === offcanvasId && !self.state.animating) {

                self.$ctx.trigger('offcanvasToggled');

                // Block menu toggling for the duration of the animation
                self.state.animating = true;
                setTimeout(function () {
                    self.state.animating = false;
                }, self.settings.animationSpeed);
            }
        };

        this.toggleState = function () {
            var self = this;
            self.state.isOpen = self.state.isOpen ? false : true;
        }
    };
})(Tc.$);
