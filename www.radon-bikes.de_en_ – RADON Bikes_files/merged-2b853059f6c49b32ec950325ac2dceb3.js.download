$(document).ready(function(){
    
    console.log('jouda event-calendar');

    //
    // JQUERY UI CALENDAR
    // ------------------------------------------------------

    $.datepicker.regional['de'] = {clearText: 'löschen', clearStatus: 'aktuelles Datum löschen',
        closeText: 'schließen', closeStatus: 'ohne Änderungen schließen',
        prevText: '<zurück', prevStatus: 'letzten Monat zeigen',
        nextText: 'Vor>', nextStatus: 'nächsten Monat zeigen',
        currentText: 'heute', currentStatus: '',
        monthNames: ['Januar','Februar','März','April','Mai','Juni',
            'Juli','August','September','Oktober','November','Dezember'],
        monthNamesShort: ['Jan','Feb','Mär','Apr','Mai','Jun',
            'Jul','Aug','Sep','Okt','Nov','Dez'],
        monthStatus: 'anderen Monat anzeigen', yearStatus: 'anderes Jahr anzeigen',
        weekHeader: 'Wo', weekStatus: 'Woche des Monats',
        dayNames: ['Sonntag','Montag','Dienstag','Mittwoch','Donnerstag','Freitag','Samstag'],
        dayNamesShort: ['So','Mo','Di','Mi','Do','Fr','Sa'],
        dayNamesMin: ['So','Mo','Di','Mi','Do','Fr','Sa'],
        dayStatus: 'Setze DD als ersten Wochentag', dateStatus: 'Wähle D, M d',
        dateFormat: 'dd.mm.yy', firstDay: 1,
        initStatus: 'Wähle ein Datum', isRTL: false};

    if($('html[lang="de"]').length){
        $.datepicker.setDefaults($.datepicker.regional['de']);
    }

    $( "#module-homepage-calendar" ).datepicker({
        inline: true,
        minDate: '-1m',
        maxDate: '+1y +1m',
        beforeShowDay: function (date) {
            var m = date.getMonth(),
                d = date.getDate(),
                y = date.getFullYear();
            var nowDate = new Date(y,m,d,0,0,0);
            var eventCount = 0;
            var eventTitles = [];
            $('.event-calendar-date').each(function(){
                var $this = $(this);
                var start = $this.data('calendar-eventstart');
                var startSplit = start.split('-');
                var startY = startSplit[0];
                var startM = startSplit[1];
                var startD = startSplit[2];
                var startDate = new Date(startY,startM-1,startD,0,0,0);

                var end = $this.data('calendar-eventend');
                var endSplit = end.split('-');
                var endY = endSplit[0];
                var endM = endSplit[1];
                var endD = endSplit[2];
                var endDate = new Date(endY,endM-1,endD,0,0,0);
                //add start date
                if(nowDate.getTime() == startDate.getTime()){
                    eventCount++;
                    eventTitles.push($this.data('calendar-title'));
                }
                else if(startDate.getTime() < nowDate.getTime() && endDate.getTime() >= nowDate.getTime()){
                    eventCount++;
                    eventTitles.push($this.data('calendar-title'));
                }
            });
            if(eventCount > 0){
                if(eventCount > 3) eventCount = 3;
                return [true, 'event-active event-count-'+eventCount, eventTitles.join(',')];
            }
            return [true,''];
        },
        onSelect: function(dateText, inst) {
            var nowDate = new Date(inst['currentYear'],inst['currentMonth'],inst['currentDay'],0,0,0);
            $('.event-calendar-date').each(function(){
                var $this = $(this);
                var start = $this.data('calendar-eventstart');
                var startSplit = start.split('-');
                var startY = startSplit[0];
                var startM = startSplit[1];
                var startD = startSplit[2];
                var startDate = new Date(startY,startM-1,startD,0,0,0);

                var end = $this.data('calendar-eventend');
                var endSplit = end.split('-');
                var endY = endSplit[0];
                var endM = endSplit[1];
                var endD = endSplit[2];
                var endDate = new Date(endY,endM-1,endD,0,0,0);
                //add start date
                if(nowDate.getTime() == startDate.getTime()){
                    $this.show();
                }
                else if(startDate.getTime() < nowDate.getTime() && endDate.getTime() >= nowDate.getTime()){
                    $this.show();
                } else {
                    $this.hide();
                }
            });
        }
    });

    function resizeEventCalendar(){
        var li = $('#home-event-wrapper').closest('li');
        var h = li.prev().find('.bg-black').height();
        li.find('.bg-black').css({minHeight:h});
    }
    $(window).on('resize',function(){
        resizeEventCalendar();
    });
    resizeEventCalendar();
});
